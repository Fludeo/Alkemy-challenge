{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["eslint:recommended","plugin:jsdoc/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "jsdoc/valid-types":0,
        "jsdoc/require-param-description":0
    },
    "plugins": [
        "jsdoc"
    ]
}
